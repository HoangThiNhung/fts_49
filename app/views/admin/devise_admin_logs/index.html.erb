<% provide :title, t("admin.logs.see_log") %>
<div class="col-lg-3">
  <%= render "layouts/side_bar" %>
</div>
<div class="col-lg-9">
  <h1><%= t "admin.logs.see_log" %></h1>
  <div class="row">
    <table class="table">
      <tr>
        <td><%= t "logs.name" %></td>
        <td><%= t "logs.action" %></td>
        <td><%= t "logs.time" %></td>
      </tr>
      <% @devise_admin_logs.each do |log| %>
        <tr>
          <td><%= log.user.name %></td>
          <td><%= log.action %></td>
          <td><%= log.created_at %></td>
        </tr>
      <% end %>
    </table>
  </div>
  <%= paginate @devise_admin_logs %><br>
  <%= page_entries_info @devise_admin_logs %>
</div>